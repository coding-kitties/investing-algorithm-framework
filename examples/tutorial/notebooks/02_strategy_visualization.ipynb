    "   \"source\": [\n",
    "    \"# Strategy Visualization\\n\",\n",
    "    \"\\n\",\n",
    "    \"This notebook gives an initial visualization of the trades made by the EMA Crossover RSI Confirmation Strategy during backtesting.\"\n",
    "   ],\n",
    "   \"id\": \"452063c5e1c31f59\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"source\": \"## Constants\",\n",
    "   \"id\": \"3052c181857be643\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"from pathlib import Path\\n\",\n",
    "    \"from datetime import datetime, timezone\\n\",\n",
    "    \"from investing_algorithm_framework import BacktestDateRange, \\\\\\n\",\n",
    "    \"    select_backtest_date_ranges\\n\",\n",
    "    \"\\n\",\n",
    "    \"data_storage_path = Path.cwd() / \\\"data\\\"\\n\",\n",
    "    \"backtest_results_dir = Path.cwd() / \\\"backtest_results\\\"\\n\",\n",
    "    \"backtest_window_date_range = BacktestDateRange(\\n\",\n",
    "    \"    start_date=datetime(2022, 1, 1, tzinfo=timezone.utc),\\n\",\n",
    "    \"    end_date=datetime(2025, 12, 30, tzinfo=timezone.utc)\\n\",\n",
    "    \")\\n\",\n",
    "    \"MARKET = \\\"BITVAVO\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"in_sample_assets = [\\\"BTC\\\", \\\"ETH\\\", \\\"ADA\\\", \\\"SOL\\\", \\\"DOT\\\"]\\n\",\n",
    "    \"out_sample_assets = [\\\"XRP\\\", \\\"LTC\\\", \\\"BCH\\\"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"rolling_backtest_windows = select_backtest_date_ranges(\\n\",\n",
    "    \"    data=in_sample_data[\\\"BTC\\\"],\\n\",\n",
    "    \"    start_date=backtest_window_date_range.start_date,\\n\",\n",
    "    \"    end_date=backtest_window_date_range.end_date,\\n\",\n",
    "    \"    train_days=365,\\n\",\n",
    "    \"    test_days=180,\\n\",\n",
    "    \"    gap_days=30,\\n\",\n",
    "    \"    step_days=90,\\n\",\n",
    "    \")\"\n",
    "   ],\n",
    "   \"id\": \"e5fd52bdc6969dcd\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"source\": \"## Setup folder structure\",\n",
    "   \"id\": \"888167da69674e5f\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"import os\\n\",\n",
    "    \"\\n\",\n",
    "    \"# create all required directories\\n\",\n",
    "    \"if not os.path.exists(data_storage_path):\\n\",\n",
    "    \"    os.makedirs(data_storage_path)\\n\",\n",
    "    \"\\n\",\n",
    "    \"if not os.path.exists(backtest_results_dir):\\n\",\n",
    "    \"    os.makedirs(backtest_results_dir)\"\n",
    "   ],\n",
    "   \"id\": \"5083c726194cfc4f\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"source\": \"## Data downloading\",\n",
    "   \"id\": \"2a474b03ea2a314b\"\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"id\": \"initial_id\",\n",
    "   \"metadata\": {\n",
    "    \"collapsed\": true\n",
    "   },\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"from investing_algorithm_framework import download\\n\",\n",
    "    \"in_sample_data = {}\\n\",\n",
    "    \"\\n\",\n",
    "    \"for symbol in in_sample_assets:\\n\",\n",
    "    \"    symbol_pair = f\\\"{symbol}/EUR\\\"\\n\",\n",
    "    \"    in_sample_data[symbol] = download(\\n\",\n",
    "    \"        symbol=symbol_pair,\\n\",\n",
    "    \"        market=MARKET,\\n\",\n",
    "    \"        time_frame=\\\"1d\\\",\\n\",\n",
    "    \"        data_type=\\\"ohlcv\\\",\\n\",\n",
    "    \"        start_date=backtest_window_date_range.start_date,\\n\",\n",
    "    \"        end_date=backtest_window_date_range.end_date,\\n\",\n",
    "    \"        save=True,\\n\",\n",
    "    \"        storage_path=str(data_storage_path)\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"    first_date = in_sample_data[symbol].index[0]\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if first_date > backtest_window_date_range.start_date:\\n\",\n",
    "    \"        print(f\\\"Warning: Data for {symbol_pair} starts on {first_date} which is after the requested start date of {backtest_window_date_range.start_date}.\\\")\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"source\": \"## Prameters and Strategy Initialization\",\n",
    "   \"id\": \"849777ea301750da\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"params = {\\n\",\n",
    "    \"    \\\"trading_symbol\\\": \\\"EUR\\\",\\n\",\n",
    "    \"    \\\"macd_short_period\\\": 12,\\n\",\n",
    "    \"    \\\"macd_long_period\\\": 26,\\n\",\n",
    "    \"    \\\"macd_signal_period\\\": 9,\\n\",\n",
    "    \"    \\\"close_timeframe\\\": \\\"2h\\\",\\n\",\n",
    "    \"    \\\"macd_timeframe\\\": \\\"2h\\\",\\n\",\n",
    "    \"    \\\"rsi_timeframe\\\": \\\"2h\\\",\\n\",\n",
    "    \"    \\\"rsi_period\\\": 14,\\n\",\n",
    "    \"    \\\"rsi_overbought_threshold\\\": 75,\\n\",\n",
    "    \"    \\\"rsi_oversold_threshold\\\": 30,\\n\",\n",
    "    \"    \\\"rsi_lookback_window_size\\\": 12, # 24 hours\\n\",\n",
    "    \"    \\\"number_of_neighbors_to_compare\\\": 3,\\n\",\n",
    "    \"    \\\"min_consecutive\\\": 2,\\n\",\n",
    "    \"    \\\"divergence_lookback_window_size\\\": 12, # 24 hours\\n\",\n",
    "    \"}\"\n",
    "   ],\n",
    "   \"id\": \"19b2a464a756621c\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"from investing_algorithm_framework import TimeUnit\\n\",\n",
    "    \"algorithm_id = \\\"visualization_strategy\\\"\\n\",\n",
    "    \"strategy = Strategy(\\n\",\n",
    "    \"    **params,\\n\",\n",
    "    \"    algorithm_id=algorithm_id,\\n\",\n",
    "    \"    time_unit=TimeUnit.HOUR,\\n\",\n",
    "    \"    interval=2,\\n\",\n",
    "    \"    market=MARKET,\\n\",\n",
    "    \"    symbols=in_sample_assets,\\n\",\n",
    "    \")\"\n",
    "   ],\n",
    "   \"id\": \"fc5c3403db937cc8\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"backtest_run = backtest.get_backtest_run(first_train_window)\\n\",\n",
    "    \"trades = backtest_run.get_trades()\\n\",\n",
    "    \"print(len(trades))\\n\",\n",
    "    \"\\n\",\n",
    "    \"buy_orders = backtest_run.get_orders(order_side=\\\"buy\\\")\\n\",\n",
    "    \"sell_orders = backtest_run.get_orders(order_side=\\\"sell\\\")\"\n",
    "   ],\n",
    "   \"id\": \"98b802a4fa9707e2\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"import plotly.graph_objects as go\\n\",\n",
    "    \"from plotly.subplots import make_subplots\\n\",\n",
    "    \"from investing_algorithm_framework import download\\n\",\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Get orders from backtest run\\n\",\n",
    "    \"buy_orders = backtest_run.get_orders(order_side=\\\"buy\\\")\\n\",\n",
    "    \"sell_orders = backtest_run.get_orders(order_side=\\\"sell\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"def plot_orders_for_symbol(symbol: str, buy_orders: list, sell_orders: list, start_date, end_date, timeframe: str = \\\"2h\\\"):\\n\",\n",
    "    \"    \\\"\\\"\\\"Plot candlestick chart with order markers for a specific symbol\\\"\\\"\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Download OHLCV data for the symbol\\n\",\n",
    "    \"    ohlcv = download(\\n\",\n",
    "    \"        market=\\\"BITVAVO\\\",\\n\",\n",
    "    \"        symbol=f\\\"{symbol}/EUR\\\",\\n\",\n",
    "    \"        time_frame=timeframe,\\n\",\n",
    "    \"        start_date=start_date,\\n\",\n",
    "    \"        end_date=end_date,\\n\",\n",
    "    \"        storage_path=str(data_storage_path),\\n\",\n",
    "    \"        pandas=True\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Filter orders for this symbol\\n\",\n",
    "    \"    symbol_buy_orders = [o for o in buy_orders if o.target_symbol == symbol]\\n\",\n",
    "    \"    symbol_sell_orders = [o for o in sell_orders if o.target_symbol == symbol]\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Create figure with secondary y-axis for volume\\n\",\n",
    "    \"    fig = make_subplots(\\n\",\n",
    "    \"        rows=2, cols=1,\\n\",\n",
    "    \"        shared_xaxes=True,\\n\",\n",
    "    \"        vertical_spacing=0.03,\\n\",\n",
    "    \"        row_heights=[0.8, 0.2],\\n\",\n",
    "    \"        subplot_titles=(f'{symbol}/EUR Price with Orders', 'Volume')\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Add candlestick chart\\n\",\n",
    "    \"    fig.add_trace(\\n\",\n",
    "    \"        go.Candlestick(\\n\",\n",
    "    \"            x=ohlcv.index,\\n\",\n",
    "    \"            open=ohlcv['Open'],\\n\",\n",
    "    \"            high=ohlcv['High'],\\n\",\n",
    "    \"            low=ohlcv['Low'],\\n\",\n",
    "    \"            close=ohlcv['Close'],\\n\",\n",
    "    \"            name='OHLCV',\\n\",\n",
    "    \"            increasing_line_color='#26a69a',\\n\",\n",
    "    \"            decreasing_line_color='#ef5350'\\n\",\n",
    "    \"        ),\\n\",\n",
    "    \"        row=1, col=1\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Add buy markers\\n\",\n",
    "    \"    if symbol_buy_orders:\\n\",\n",
    "    \"        buy_dates = [o.created_at for o in symbol_buy_orders]\\n\",\n",
    "    \"        buy_prices = [o.price for o in symbol_buy_orders]\\n\",\n",
    "    \"        fig.add_trace(\\n\",\n",
    "    \"            go.Scatter(\\n\",\n",
    "    \"                x=buy_dates,\\n\",\n",
    "    \"                y=buy_prices,\\n\",\n",
    "    \"                mode='markers',\\n\",\n",
    "    \"                name='Buy',\\n\",\n",
    "    \"                marker=dict(\\n\",\n",
    "    \"                    symbol='triangle-up',\\n\",\n",
    "    \"                    size=15,\\n\",\n",
    "    \"                    color='#00c853',\\n\",\n",
    "    \"                    line=dict(width=2, color='white')\\n\",\n",
    "    \"                ),\\n\",\n",
    "    \"                hovertemplate='<b>BUY</b><br>Date: %{x}<br>Price: €%{y:.2f}<extra></extra>'\\n\",\n",
    "    \"            ),\\n\",\n",
    "    \"            row=1, col=1\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Add sell markers\\n\",\n",
    "    \"    if symbol_sell_orders:\\n\",\n",
    "    \"        sell_dates = [o.created_at for o in symbol_sell_orders]\\n\",\n",
    "    \"        sell_prices = [o.price for o in symbol_sell_orders]\\n\",\n",
    "    \"        fig.add_trace(\\n\",\n",
    "    \"            go.Scatter(\\n\",\n",
    "    \"                x=sell_dates,\\n\",\n",
    "    \"                y=sell_prices,\\n\",\n",
    "    \"                mode='markers',\\n\",\n",
    "    \"                name='Sell',\\n\",\n",
    "    \"                marker=dict(\\n\",\n",
    "    \"                    symbol='triangle-down',\\n\",\n",
    "    \"                    size=15,\\n\",\n",
    "    \"                    color='#ff1744',\\n\",\n",
    "    \"                    line=dict(width=2, color='white')\\n\",\n",
    "    \"                ),\\n\",\n",
    "    \"                hovertemplate='<b>SELL</b><br>Date: %{x}<br>Price: €%{y:.2f}<extra></extra>'\\n\",\n",
    "    \"            ),\\n\",\n",
    "    \"            row=1, col=1\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Add volume bars\\n\",\n",
    "    \"    colors = ['#26a69a' if close >= open else '#ef5350'\\n\",\n",
    "    \"              for close, open in zip(ohlcv['Close'], ohlcv['Open'])]\\n\",\n",
    "    \"    fig.add_trace(\\n\",\n",
    "    \"        go.Bar(\\n\",\n",
    "    \"            x=ohlcv.index,\\n\",\n",
    "    \"            y=ohlcv['Volume'],\\n\",\n",
    "    \"            name='Volume',\\n\",\n",
    "    \"            marker_color=colors,\\n\",\n",
    "    \"            opacity=0.5\\n\",\n",
    "    \"        ),\\n\",\n",
    "    \"        row=2, col=1\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Update layout\\n\",\n",
    "    \"    fig.update_layout(\\n\",\n",
    "    \"        title=f'{symbol}/EUR Trading Activity ({len(symbol_buy_orders)} buys, {len(symbol_sell_orders)} sells)',\\n\",\n",
    "    \"        xaxis_rangeslider_visible=False,\\n\",\n",
    "    \"        template='plotly_dark',\\n\",\n",
    "    \"        height=700,\\n\",\n",
    "    \"        showlegend=True,\\n\",\n",
    "    \"        legend=dict(\\n\",\n",
    "    \"            yanchor=\\\"top\\\",\\n\",\n",
    "    \"            y=0.99,\\n\",\n",
    "    \"            xanchor=\\\"left\\\",\\n\",\n",
    "    \"            x=0.01\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    fig.update_yaxes(title_text=\\\"Price (EUR)\\\", row=1, col=1)\\n\",\n",
    "    \"    fig.update_yaxes(title_text=\\\"Volume\\\", row=2, col=1)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    return fig\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Plot orders for each asset\\n\",\n",
    "    \"for symbol in in_sample_assets:\\n\",\n",
    "    \"    symbol_buys = [o for o in buy_orders if o.target_symbol == symbol]\\n\",\n",
    "    \"    symbol_sells = [o for o in sell_orders if o.target_symbol == symbol]\\n\",\n",
    "    \"    if symbol_buys or symbol_sells:\\n\",\n",
    "    \"        fig = plot_orders_for_symbol(\\n\",\n",
    "    \"            symbol=symbol,\\n\",\n",
    "    \"            buy_orders=buy_orders,\\n\",\n",
    "    \"            sell_orders=sell_orders,\\n\",\n",
    "    \"            start_date=first_train_window.start_date,\\n\",\n",
    "    \"            end_date=first_train_window.end_date,\\n\",\n",
    "    \"            timeframe=params['close_timeframe']\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"        fig.show()\"\n",
    "   ],\n",
    "   \"id\": \"aedeb9987810fa10\"\n",
    "  }\n",
    " ],\n",
